<grammar version="1.0" xml:lang="pl-PL" root="rootRule" xmlns="http://www.w3.org/2001/06/grammar"
         tag-format="semantics/1.0">
  <rule id="rootRule">
    <ruleref uri="#action"></ruleref>
    <tag>out.action=rules.latest();</tag>
    <ruleref uri="#Number"></ruleref>
    <tag>out.first=rules.latest();</tag>
    <ruleref uri="#Number"></ruleref>
    <tag>out.second=rules.latest();</tag>
    <ruleref uri="#Number"></ruleref>
    <tag>out.third=rules.latest();</tag>
    <ruleref uri="#Number"></ruleref>
    <tag>out.fourth=rules.latest();</tag>
    <ruleref uri="#Number"></ruleref>
    <tag>out.fifth=rules.latest();</tag>
    <ruleref uri="#Number"></ruleref>
    <tag>out.sixth=rules.latest();</tag>
    <ruleref uri="#Number"></ruleref>
    <tag>out.seventh=rules.latest();</tag>
    <ruleref uri="#Number"></ruleref>
    <tag>out.eighth=rules.latest();</tag>
    <ruleref uri="#Number"></ruleref>
    <tag>out.ninth=rules.latest();</tag>
    <ruleref uri="#Number"></ruleref>
    <tag>out.tenth=rules.latest();</tag>
    <ruleref uri="#Number"></ruleref>
    <tag>out.eleventh=rules.latest();</tag>
  </rule>

  <rule id="action">
    <one-of>
      <item>
        oddać <tag>out="oddac"</tag>
      </item>
      <item>
        oddanie <tag>out="oddac"</tag>
      </item>
      <item>
        wypożyczyć <tag>out="wypozyczyc"</tag>
      </item>
      <item>
        wypożeczenie <tag>out="wypozyczyc"</tag>
      </item>
      <item>
         <tag>out="none"</tag>
      </item>
    </one-of>
  </rule>
  <rule id="Number">
    <one-of>
      <item>
        jeden <tag>out="1"</tag>
      </item>
      <item>
        dwa <tag>out="2"</tag>
      </item>
      <item>
        trzy <tag>out="3"</tag>
      </item>
      <item>
        cztery <tag>out="4"</tag>
      </item>
      <item>
        pięć <tag>out="5"</tag>
      </item>
      <item>
        sześć <tag>out="6"</tag>
      </item>
      <item>
        siedem <tag>out="7"</tag>
      </item>
      <item>
        osiem <tag>out="8"</tag>
      </item>
      <item>
        dziewięć <tag>out="9"</tag>
      </item>
      <item>
        zero <tag>out="0"</tag>
      </item>
      <item>
         <tag>out="none"</tag>
      </item>
    </one-of>
  </rule>

</grammar>

